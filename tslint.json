{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": false,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "arrow-parens": false,
    "no-console": [false, "log", "error"],
    "trailing-comma": false,
    "space-before-function-paren": true,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  },
  "rulesDirectory": []
}
